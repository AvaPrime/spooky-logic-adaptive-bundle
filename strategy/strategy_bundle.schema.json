{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Spooky Strategy Bundle",
  "type": "object",
  "required": [
    "id",
    "version",
    "playbooks",
    "policies",
    "signature"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "playbooks": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "policies": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "signature": {
      "type": "string"
    }
  }
}